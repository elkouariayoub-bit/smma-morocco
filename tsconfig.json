{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["dom", "dom.iterable", "es2020"],
    "allowJs": false,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["*"],
      "@/components/*": ["./components/*"],
      "@/lib/*": ["./lib/*"],
      "@daveyplate/better-auth-ui": ["./better-auth-ui"],
      "better-auth": ["./better-auth"],
      "better-auth/providers": ["./better-auth/providers.ts"],
      "better-auth/next-js": ["./better-auth/next-js.ts"],
      "better-auth/client": ["./better-auth/client.ts"],
      "@supabase/ssr": ["./stubs/supabase-ssr.ts"],
      "next-themes": ["./stubs/next-themes.tsx"],
      "react-hook-form": ["./stubs/react-hook-form.ts"],
      "clsx": ["./stubs/clsx.ts"],
      "tailwind-merge": ["./stubs/tailwind-merge.ts"],
      "class-variance-authority": ["./stubs/class-variance-authority.ts"],
      "@radix-ui/react-popover": ["./stubs/radix-popover.tsx"],
      "@radix-ui/react-slot": ["./stubs/radix-slot.tsx"],
      "@radix-ui/react-scroll-area": ["./stubs/radix-scroll-area.tsx"],
      "@radix-ui/react-dialog": ["./stubs/radix-dialog.tsx"],
      "geist/font/sans": ["./stubs/geist-font-sans.ts"],
      "geist/font/mono": ["./stubs/geist-font-mono.ts"],
      "@vercel/analytics/next": ["./stubs/vercel-analytics.tsx"],
      "framer-motion": ["./stubs/framer-motion.tsx"],
      "@tanstack/react-table": ["./stubs/tanstack-react-table.ts"],
      "recharts": ["./stubs/recharts.tsx"],
      "zustand": ["./stubs/zustand.ts"],
      "zod": ["./stubs/zod.ts"],
      "date-fns": ["./stubs/date-fns.ts"],
      "swr": ["./stubs/swr.ts"],
      "tailwindcss": ["./stubs/tailwindcss.css"],
      "tw-animate-css": ["./stubs/tw-animate-css.css"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
